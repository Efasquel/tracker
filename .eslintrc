{
  "env": {
    "browser": true,
    "es2021": true,
    "node": false
  },
  "extends": ["airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier", "react"],
  "rules": {
    "semi": ["error", "always"],
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": "error",
    "global-require": "off"
  }
}
